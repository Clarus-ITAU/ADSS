[Unit]
Description=mhddos_proxy
After=network.target

[Service]
Type=simple
Restart=on-failure
RestartSec=10
ExecStart=/opt/itarmy/bin/mhddos_proxy_linux --lang de --copies 12 --threads 5000 --source adss
WorkingDirectory=/opt/itarmy/bin
SyslogIdentifier=mhddos
SyslogFacility=daemon

[Install]
WantedBy=multi-user.target